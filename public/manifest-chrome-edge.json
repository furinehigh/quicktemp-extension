{
  "manifest_version": 3,
  "name": "QuickTemp - Highly customizable temp mail UI",
  "version": "1.0",
  "description": "An advanced disposalbe email UI for pros",
  "icons": {
    "61": "icon61.png"
  },
  "action": { "default_popup": "index.html" },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "notifications",
    "unlimitedStorage"
  ]
}