{
  "manifest_version": 2,
  "name": "QuickTemp - Highly customizable temp mail UI",
  "version": "1.0",
  "description": "An advanced disposalbe email UI for pros",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "61": "icon61.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "popup.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "storage",
    "notifications",
    "unlimitedStorage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "furinehigh@outlook.com",
      "strict_min_version": "109.0"
    }
  }
}